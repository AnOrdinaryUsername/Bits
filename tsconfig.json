{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "allowSyntheticDefaultImports": true,
    "module": "es6",
    "target": "es2016",
    "lib": [
      "es2017",
      "dom",
      "ScriptHost"
    ],
    "jsx": "react",
    "allowJs": true,
    "noEmit": true
  }
}